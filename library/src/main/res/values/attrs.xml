<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="fill_color" format="color" />
        <attr name="circleBackgroundColor" format="color" />
    </declare-styleable>

    <attr name="strokeColor" format="color" />
    <attr name="speed">
        <enum name="speedOne" value="1" />
        <enum name="speedTwo" value="2" />
    </attr>

    <declare-styleable name="RightDiaView">
        <attr name="strokeColor" />
        <attr name="speed" />
    </declare-styleable>

    <declare-styleable name="WrongDiaView">
        <attr name="strokeColor" />
        <attr name="speed" />
    </declare-styleable>
</resources>